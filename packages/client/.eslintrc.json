{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "import/order": [
      "error",
      {
        "distinctGroup": true,
        "newlines-between": "always",
        "groups": [
          "builtin",
          "external",
          "internal",
          [
            "parent",
            "sibling"
          ],
          "index",
          "object",
          "type"
        ]
      }
    ]
  }
}
